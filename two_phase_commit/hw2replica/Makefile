ifeq "$(CFG)" "Debug"
OUTDIR=Debug
OUTFILE=$(OUTDIR)/hw2replica
CFG_INC=-I../db -I../thrift/gen-cpp 
CFG_LIB=-lsqlite3 -lthrift 
CFG_OBJ=
COMMON_OBJ=$(OUTDIR)/Master.o $(OUTDIR)/Replica.o $(OUTDIR)/db.o \
	$(OUTDIR)/hw2replica.o $(OUTDIR)/schema.o \
	$(OUTDIR)/services_constants.o $(OUTDIR)/services_types.o 
OBJ=$(COMMON_OBJ) $(CFG_OBJ)
ALL_OBJ=$(OUTDIR)/Master.o $(OUTDIR)/Replica.o $(OUTDIR)/db.o \
	$(OUTDIR)/hw2replica.o $(OUTDIR)/schema.o \
	$(OUTDIR)/services_constants.o $(OUTDIR)/services_types.o -lsqlite3 \
	-lthrift 

COMPILE=g++ -c  "-DHAVE_INTTYPES_H" "-DHAVE_NETINET_IN_H" -g -std=c++0x -Werror -Wall -o "$(OUTDIR)/$(*F).o" $(CFG_INC) $<
LINK=g++  -g -o "$(OUTFILE)" $(ALL_OBJ)

# Pattern rules
$(OUTDIR)/%.o : %.cpp
	$(COMPILE)

$(OUTDIR)/%.o : ../thrift/gen-cpp/%.cpp
	$(COMPILE)

$(OUTDIR)/%.o : ../db/%.cpp
	$(COMPILE)

# Build rules
all: $(OUTFILE)

$(OUTFILE): $(OUTDIR)  $(OBJ)
	$(LINK)

$(OUTDIR):
	$(MKDIR) -p "$(OUTDIR)"

# Rebuild this project
rebuild: cleanall all

# Clean this project
clean:
	$(RM) -f $(OUTFILE)
	$(RM) -f $(OBJ)

# Clean this project and all dependencies
cleanall: clean
endif

